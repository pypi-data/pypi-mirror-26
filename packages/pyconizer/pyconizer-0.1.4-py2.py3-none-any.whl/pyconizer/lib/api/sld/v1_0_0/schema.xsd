    </xsd:sequence>
  </xsd:complexType>
  <!--<include xmlns="http://www.w3.org/2001/XMLSchema" xmlns:gml="http://www.opengis.net/gml" xmlns:xlink="http://www.w3.org/1999/xlink" schemaLocation="feature.xsd"/>
