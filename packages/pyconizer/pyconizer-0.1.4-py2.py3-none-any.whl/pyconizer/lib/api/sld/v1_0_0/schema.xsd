    -->
<xsd:complexType name="UnaryLogicOpType">
        <xsd:complexContent>
            <xsd:extension base="ogc:LogicOpsType">
                <xsd:sequence>
                    <xsd:choice>
                        <xsd:element ref="ogc:comparisonOps"/>
                        <xsd:element ref="ogc:spatialOps"/>
                        <xsd:element ref="ogc:logicOps"/>
                    </xsd:choice>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
