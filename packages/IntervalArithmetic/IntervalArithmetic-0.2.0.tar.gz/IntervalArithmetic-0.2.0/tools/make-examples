#! /usr/bin/bash

# Regenerate from scratch
examples=doc/sphinx/source/examples
rm -rf ${examples}

echo
echo Generate RST examples files
# ./bin/generate-rst-examples --skip-external-figure --skip-figure
./bin/generate-rst-examples # --force

echo
echo Run Sphinx
pushd doc/sphinx/
./make-html # --clean
popd
