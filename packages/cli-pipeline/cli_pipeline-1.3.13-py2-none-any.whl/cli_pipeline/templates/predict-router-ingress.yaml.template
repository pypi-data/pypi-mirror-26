apiVersion: extensions/v1beta1
kind: Ingress
metadata:
  name: gateway
  annotations:
    kubernetes.io/ingress.class: "istio"
spec:
  rules:
  - http:
      paths:
      - path: /api/v1/model/predict/tensorflow/linear/.*
        backend:
          serviceName: predict-tensorflow-linear 
          servicePort: 9040
