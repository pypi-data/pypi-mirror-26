   "source": [
    "paths = molpx.generate.MEP_auto([iY[:,[0,1]] for iY in Y], MD_trajfiles, \n",
    "                                resolution=1000,\n",
    "                                step_size=5,\n",
    "                                endpoints=None, \n",
    "                                top=top) "
   ]
  },
  {
