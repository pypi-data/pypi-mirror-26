   "source": [
    "centers = []\n",
    "for ii, ival in paths.items():\n",
    "    for jj, jval in ival.items():\n",
    "        print(ii,jj)\n",
    "        centers.append([jval[\"proj\"][0, 0], jval[\"proj\"][0, 1]])\n",
    "        break\n",
    "centers = np.vstack(centers)\n"
   ]
  },
