#!/home/cornish/dev/envs/flextime/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'flextime==1.1','console_scripts','ft'
__requires__ = 'flextime==1.1'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('flextime==1.1', 'console_scripts', 'ft')()
    )
