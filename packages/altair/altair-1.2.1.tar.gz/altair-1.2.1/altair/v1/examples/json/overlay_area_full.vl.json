{
    "data": {
        "format": {
            "type": "csv"
        },
        "url": "https://vega.github.io/vega-lite/data/stocks.csv"
    },
    "description": "Google's stock price over time.",
    "layers": [
        {
            "encoding": {
                "x": {
                    "field": "date",
                    "type": "temporal"
                },
                "y": {
                    "field": "price",
                    "type": "quantitative"
                }
            },
            "mark": "area"
        },
        {
            "encoding": {
                "x": {
                    "field": "date",
                    "type": "temporal"
                },
                "y": {
                    "field": "price",
                    "type": "quantitative"
                }
            },
            "mark": "line"
        },
        {
            "config": {
                "mark": {
                    "filled": true
                }
            },
            "encoding": {
                "x": {
                    "field": "date",
                    "type": "temporal"
                },
                "y": {
                    "field": "price",
                    "type": "quantitative"
                }
            },
            "mark": "point"
        }
    ],
    "transform": {
        "filter": "datum.symbol==='GOOG'"
    }
}