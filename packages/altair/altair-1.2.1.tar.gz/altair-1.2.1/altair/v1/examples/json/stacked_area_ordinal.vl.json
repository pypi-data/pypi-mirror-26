{
    "config": {
        "mark": {
            "interpolate": "monotone"
        }
    },
    "data": {
        "url": "https://vega.github.io/vega-lite/data/cars.json"
    },
    "encoding": {
        "color": {
            "field": "Cylinders",
            "type": "ordinal"
        },
        "x": {
            "field": "Year",
            "timeUnit": "year",
            "type": "temporal"
        },
        "y": {
            "aggregate": "sum",
            "field": "Weight_in_lbs",
            "type": "quantitative"
        }
    },
    "mark": "area"
}