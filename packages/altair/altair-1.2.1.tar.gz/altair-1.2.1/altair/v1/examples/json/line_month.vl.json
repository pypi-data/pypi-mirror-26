{
    "config": {
        "mark": {
            "interpolate": "monotone"
        }
    },
    "data": {
        "format": {
            "type": "csv"
        },
        "url": "https://vega.github.io/vega-lite/data/seattle-temps.csv"
    },
    "encoding": {
        "x": {
            "axis": {
                "shortTimeLabels": true
            },
            "field": "date",
            "timeUnit": "month",
            "type": "temporal"
        },
        "y": {
            "aggregate": "mean",
            "field": "temp",
            "type": "quantitative"
        }
    },
    "mark": "line"
}