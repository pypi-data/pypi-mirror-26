{
    "data": {
        "url": "https://vega.github.io/vega-lite/data/stocks.csv"
    },
    "description": "Stock price average broken down by quarter.",
    "encoding": {
        "color": {
            "field": "date",
            "timeUnit": "quarter",
            "type": "temporal"
        },
        "x": {
            "field": "date",
            "timeUnit": "year",
            "type": "temporal"
        },
        "y": {
            "aggregate": "mean",
            "field": "price",
            "type": "quantitative"
        }
    },
    "mark": "line"
}