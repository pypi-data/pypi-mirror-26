{
    "config": {
        "cell": {
            "height": 200,
            "width": 200
        }
    },
    "data": {
        "url": "https://vega.github.io/vega-lite/data/cars.json"
    },
    "encoding": {
        "x": {
            "field": "Origin",
            "scale": {
                "bandSize": "fit"
            },
            "type": "nominal"
        },
        "y": {
            "aggregate": "count",
            "field": "*",
            "type": "quantitative"
        }
    },
    "mark": "bar"
}