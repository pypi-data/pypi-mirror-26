{
    "separator": " ",
    "parser": {
        "__class__": "Parser",
        "state_sizes": [ 1 ],
        "reset_on_sentence_end": true
    },
    "scanner": {
        "__class__": "RegExpScanner",
        "expr": "(?:(?P<end>[.!?]+)|(?P<word>(?:[^\\w\\s]+|\\w+)))",
        "default_end": "."
    }
}
