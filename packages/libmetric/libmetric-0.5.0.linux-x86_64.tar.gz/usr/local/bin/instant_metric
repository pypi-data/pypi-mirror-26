#!/usr/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'libmetric==0.5.0','console_scripts','instant_metric'
__requires__ = 'libmetric==0.5.0'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('libmetric==0.5.0', 'console_scripts', 'instant_metric')()
    )
