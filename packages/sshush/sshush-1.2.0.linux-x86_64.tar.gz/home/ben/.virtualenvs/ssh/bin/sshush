#!/home/ben/.virtualenvs/ssh/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'sshush==1.2.0','console_scripts','sshush'
__requires__ = 'sshush==1.2.0'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('sshush==1.2.0', 'console_scripts', 'sshush')()
    )
